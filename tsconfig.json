  "compilerOptions": {
    "module": "ESNext",
    "moduleResolution": "bundler",
    "target": "ESNext",
    "stripInternal": true,
    "declaration": true,
    "isolatedDeclarations": true,
    "erasableSyntaxOnly": true,
